package myfreespace;

import java.util.ArrayList;

public class TomakeArrayListwithGetmethod {
	public static void main(String[] args) {
		ArrayList a = new ArrayList();
		a.add("balasai");
		a.add(007);
		a.add('d');
		a.add(true);
		a.add(null);
		a.add(5.55);
		a.add(123456789);
		
		for(int i=0; i<a.size()-1;i++) {  //for each loop used to make get method  "i" variable refers to index number
			System.out.println(a.get(i));
			
		
		
		}
	}
}
