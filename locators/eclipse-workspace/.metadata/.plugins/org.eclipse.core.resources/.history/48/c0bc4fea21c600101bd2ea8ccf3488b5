package myfreespace;

import java.util.LinkedList;

public class ToCreateLinkedList {
	public static void main(String[] args) {
		LinkedList l = new LinkedList();
		l.add(100);
		l.add("balasai");
		l.add(007);
		l.add('d');
		l.add(true);
		l.add(null);
		l.add(5.55);
		l.add(123456789);
		
		System.out.println(l.size());
		System.out.println(l);
		System.out.println(l.contains('d')); // contains methods return type is boolean
		
		for(int i=0;i>=l.size()-1;i++) {
			System.out.println(i);
		}
	}

}
